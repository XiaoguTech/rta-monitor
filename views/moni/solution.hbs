<style type="text/css">
body.pushable>.pusher{
  background: #111111;
}
</style>
<div class="ui main container" id="solutionContainer">
  {{#if show_alert}}
    <h1>报警监控</h1>
    {{#if alertPanelURL}}
        <iframe src="{{alertPanelURL}}" frameborder="0" class="solutionIframe" width="100%"></iframe>
    {{/if}}
  {{/if}}
    <h1>解决方案</h1>
    <div class="row">
        <div class="sixteen wide column" id="solutionMain">
            <h1 id="solutionMainTitle">{{result.kb_title}}</h1>
            <div class="body_text">
                {{{kb_body}}}
            </div>
        </div>
    </div>
</div>