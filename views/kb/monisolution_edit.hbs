<div class="col-md-12">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <ol class="breadcrumb">
        <li><a href="{{app_context}}/solutions">{{__ "Orgnization"}}({{result.orgID}})</a></li>
        <li class="active">{{__ "Solution edit"}}</li>
      </ol>
      <h1>{{__ "Solution edit"}}</h1>
      <form method="post" id="edit_form" action="{{app_context}}/solutions/{{result.orgID}}/update" data-toggle="validator">
        <input type="hidden" name="old_alertId" value="{{result.alertID}}" />
        <div class="form-group">
          <label>{{__ "AlertID"}}</label>
          <input type="text" class="form-control" name="new_alertId" value="{{result.alertID}}" required>
        </div>
        <div class="form-group">
          <label>{{__ "OpenKBURL"}}</label>
          <input type="text" class="form-control" name="new_openKB" value="{{result.openKBURL}}" required>
        </div>
        <div class="form-group">
          <label>{{__ "AlertPanelURL"}}</label>
          <input type="text" class="form-control" name="new_alertPanel" value="{{result.alertPanelURL}}" required>
        </div>
        <div class="form-group">
          <div class="pull-right">
            <button type="submit" class="btn btn-primary">{{__ "Update"}}</button>
          </div>
        </div>
    </div>
  </div>
</div>
