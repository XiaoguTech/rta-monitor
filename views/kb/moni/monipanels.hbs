<div class="col-md-12">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <ol class="breadcrumb">
        <li><a href="{{app_context}}/monitors">{{__ "Orgnization"}}({{result.orgId}})</a></li>
        <li><a href="{{app_context}}/monitors/{{result.orgId}}">{{__ "Category"}}({{result.category_name}})</a></li>
      </ol>
      <h1>{{__ "Panels"}} <span class="pull-right"><a href="{{app_context}}/monitors/{{result.orgId}}/{{result.category_id}}/new" class="btn btn-primary">{{__ "New panels"}}</a></span></h1>
      <ul class="list-group">
        {{#each result.metric}}
          <li class="list-group-item">
            <strong>{{__ "Panels"}}:</strong>{{this.name}}
            <span class="pull-right">
              {{#is_an_admin ../session.is_admin}}
                <a href="{{app_context}}/monitors/{{../result.orgId}}/{{../result.category_id}}/edit/{{this.name}}"><i class="fa fa-pencil" aria-hidden="true"></i></a>
                  <a href="{{app_context}}/monitors/{{../result.orgId}}/{{../result.category_id}}/delete/{{this.name}}" onclick="return confirm('{{__ "Are you sure you want to delete?"}}')"><i class="fa fa-trash-o" aria-hidden="true"></i></a>
              {{/is_an_admin}}
            </span>
          </li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>
