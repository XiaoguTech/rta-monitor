<div class="col-md-12">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <ol class="breadcrumb">
        <li class="active">{{__ "select a org to enter alert solution editing page"}}</li>
      </ol>
      <h1>{{__ "Orgnization"}}</h1>
      <ul class="list-group">
        {{#each solutions}}
          <li class="list-group-item">
            <strong>{{__ "Orgnization name"}}:</strong>
            <a href="{{app_context}}/solutions/{{this.users_name}}">{{this.users_name}}</a>
          </li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>
