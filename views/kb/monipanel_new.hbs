<div class="col-md-12">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <ol class="breadcrumb">
        <li><a href="{{app_context}}/">{{__ "Home"}}</a></li>
        <li><a href="{{app_context}}/monitors">{{__ "Orgnization"}}</a></li>
        <li><a href="{{app_context}}/monitors/{{result.orgId}}">{{__ "Category"}}</a></li>
        <li><a href="{{app_context}}/monitors/{{result.orgId}}">{{__ "Panels"}}</a></li>
        <li class="active">{{__ "New panels"}}</li>
      </ol>
      <h1>{{__ "New panels"}}</h1>
      <form method="post" id="edit_form" action="{{app_context}}/monitors/{{result.orgId}}/{{result.category_id}}/panel_insert" data-toggle="validator">
        <div class="form-group">
          <label>{{__ "Panels name"}} *</label>
          <input type="text" class="form-control" name="panels_name" required>
        </div>
        <div class="form-group">
          <label>{{__ "Panels url"}} *</label>
          <input type="text" class="form-control" name="panels_url" required>
        </div>
        <div class="form-group">
          <div class="pull-right">
            <button type="submit" class="btn btn-primary">{{__ "Create new category"}}</button>
          </div>
        </div>
    </div>
  </div>
</div>